from random import *
cont = 0 #총 탑승 승객 수
for i in range(1, 51) : # 1~50 시간 난수(승객)
    time = randrange(5,51) #5분 ~ 50분 소요 시간
    if 5 <= time <= 15 :#5분 ~ 15분 사이 승객 매치
        print("[O] {0}번째 손님 (소요시간 : {1}분)".format(i,time))
        cont += 1 #매치 승객 수
    else :
        print("[ ] {0}번째 손님 (소요시간 : {1}분)".format(i,time))

print("총 탑승 승객 : {0}분".format(cont))
